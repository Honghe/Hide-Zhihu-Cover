    "description": "Hide Zhihu Article Cover",
    "content_scripts": [
        {
            "js": ["content.js"],
            "run_at": "document_end",
            "matches": [
                "https://zhuanlan.zhihu.com/p/*"
            ]
        }
    ]
  }